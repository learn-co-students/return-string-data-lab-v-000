<%= form_for(@product) do |f| %>
	<div>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>

	<div>
		<%= f.label :price %>
		<%= f.text_field :price %>
	</div>

	<div>
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>

	<div>
		<%= f.label :inventory %>
		<%= f.text_field :inventory %>
	</div>

	<div>
		<%= f.submit %>
	</div>
<% end %>