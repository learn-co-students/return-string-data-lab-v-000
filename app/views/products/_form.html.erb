<%= form_for product, url: {action: "create"}, html: {class: "product_form"} do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :inventory %><br>
  <%= f.text_field :inventory %><br>
  <%= f.submit "Create" %>
<% end %>
