<h1>Create New Product</h1>

<%= form_for @product do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>

  <%= f.label "Description" %>
  <%= f.text_field :description %>

  <%= f.label "Price" %>
  <%= f.number_field :price %>

  <%= f.label "Inventory" %>
  <%= f.number_field :inventory %>

  <%= f.submit %>

<% end %>




