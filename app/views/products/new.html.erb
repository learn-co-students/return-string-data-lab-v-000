<h1> create new product </h1>

<%= form_for @product do |f| %>
 <%= f.label :name %><br>
 <%=f.text_field :name %><br>
 <%= f.label :description %><br>
 <%= f.text_area :description %><br>
 <%= f.label :price %><br>
 <%= f.text_field :price %><br>
 <%= f.label :inventory %><br>
 <%= f.text_field :inventory %><br></br>
 <%= f.submit %>
<% end %>