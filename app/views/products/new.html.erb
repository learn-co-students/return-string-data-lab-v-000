<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :price %>
  <%= f.text_field :price %><br>
  <%= f.label :inventory %>
  <%= f.text_field :inventory %><br>
  <%= f.label :description %>
  <%= f.text_area :description %>br
  <%= f.submit %>
<% end %>
