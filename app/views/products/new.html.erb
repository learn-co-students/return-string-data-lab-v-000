<h1>Add a new product</h1>

<%= form_for( @product ) do |f| %> 
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :inventory %>
  <%= f.text_field :inventory %>
<% end %>