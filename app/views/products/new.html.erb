<%= form_for @product do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label :price %><br>
  <%= f.number_field :price %><br><br>
  <%= f.label :description%><br>
  <%= f.text_area :description%><br><br>
  <%= f.label :inventory%><br>
  <%= f.number_field :inventory%><br><br>
  <%= f.submit %>
<% end %>