<h1> Add a New Product </h1>
<%=form_for(@product) do |f|%>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div>
   <%= f.label :price %>
   <%= f.text_field :price %>
  </div>
  <div>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.label :inventory %><br>
    <%= f.text_field :inventory %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<%end%>
