<h1>Enter an Amazing Product!</h1>
<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :inventory %>
  <%= f.number_field :inventory %>
  <%= f.submit "Create Product" %>
<% end %>
