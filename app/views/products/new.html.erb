<h1>Add a Product</h1>
<%= form_for @product do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label :description %><br>
  <%= f.text_field :description %><br><br>

  <%= f.label :inventory %><br>
  <%= f.text_field :inventory %><br><br>
  
  <%= f.submit "submit" %>
<% end %>