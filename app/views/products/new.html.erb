<h3>New Product</h3>
<%=form_for @product do |f| %>
  <br><%= f.label :name %>
  <br><%= f.text_field :name %>
  <br><%= f.label :price %>
  <br><%= f.number_field :price %>
  <br><%= f.label :description %>
  <br><%= f.text_area :description %>
  <br><%= f.label :inventory %>
  <br><%= f.number_field :inventory %>
  <br><br><%= f.submit %>
<%end%>
