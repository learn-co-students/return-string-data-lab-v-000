<h1> Add A New Product </h1>

<%= form_for(@product) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :price %>
<%= f.number_field :price %>
<br>
<%= f.label :description %>
<%= f.text_field :description %>
<br>
<%= f.label :inventory %>
<%= f.number_field :inventory %>
<br>
<%= f.submit %>
<% end %>
