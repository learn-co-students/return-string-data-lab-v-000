<h1>New Product<h1>
<%= form_for(@product) do |f| %>
<div class="field">
  <%= f.label :name %><br>
  <%= f.text_field :name%>
</div>

<div class="field">
  <%= f.label :description %><br>
  <%= f.text_field :description %>
</div>

<div class="field">
  <%= f.label :inventory %><br>
  <%= f.text_field :inventory %>
</div>

<div class="actions">
  <%= f.submit %>
</div>
<% end %>
