<h1>Add New Product</h1>

<%= form_for(@product) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :price %>
    <%= f.text_field :price %>

    <%= f.label :inventory %>
    <%= f.text_field :inventory %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit %>
<% end %>