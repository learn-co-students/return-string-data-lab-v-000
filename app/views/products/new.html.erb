<%= form_for(@product) do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
    <%= f.label :inventory %><br>
    <%= f.text_field :inventory %>
    <%= f.submit %>

<% end %>
