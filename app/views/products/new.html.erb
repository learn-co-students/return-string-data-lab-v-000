<h1>New Products</h1>
<%= form_for @product do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :price %>
	<%= f.text_field :price %>
	<br>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<br>
	<%= f.label :inventory %>
	<%= f.text_field :inventory %>
	<br>
	<%= f.submit %>
<% end %>	