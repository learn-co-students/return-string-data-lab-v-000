
<%= form_for @product do |p| %>
  <%= p.label :name %>
  <%= p.text_field :name %>
  <br>
  <%= p.label :price %>
  <%= p.text_field :price %>
  <br>
  <%= p.label :description %>
  <%= p.text_area :description %>
  <br>
  <%= p.label :inventory %>
  <%= p.text_field :inventory %>
  <br>
  <%= p.submit "Submit" %>
<% end %>
