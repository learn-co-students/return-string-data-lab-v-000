<h1>Add a New Product</h1>
<%= form_for :product, :url => products_path do |f| %>
  <%= f.label :name%>
  <%= f.text_field :name%>
  <%= f.label :price%>
  <%= f.number_field :price%>
  <%= f.label :description%>
  <%= f.text_field :description%>
  <%= f.label :inventory%>
  <%= f.number_field :inventory%>
  <%= f.submit %>
<%end%>
