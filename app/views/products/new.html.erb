<h1>Create a Product</h1>

<%= form_with(model: @product, local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :inventory %>
  <%= f.text_field :inventory %>
  <%= f.submit :Submit %>
<% end %>