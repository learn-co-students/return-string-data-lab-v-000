<h1>Add A Product</h1>
<%= form_for @product do |f| %>
  <%= f.label :name%>
  <%= f.text_field :name%><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :price %>
  <%= f.text_field :price%><br>
  <%= f.label :inventory %>
  <%= f.number_field :inventory%><br>
  <%= f.submit %>
<%end%>
